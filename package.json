{
  "name": "dhora.app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx lint-staged",
    "tsc": "bash -c tsc --noEmit",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint:staged": "eslint",
    "migration:generate": "drizzle-kit generate:pg --schema=./src/db/schema.ts",
    "migration:push": "node -r esbuild-register src/db/migrate.ts",
    "migrate": "drizzle-kit generate:pg --schema=./src/db/schema.ts && node -r esbuild-register src/db/migrate.ts",
    "db:seed": "node -r esbuild-register src/db/seed.ts",
    "docker-build": "docker build -t app .",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@reduxjs/toolkit": "^2.1.0",
    "@sendgrid/mail": "^8.1.0",
    "@t3-oss/env-core": "^0.8.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.321.0",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.0",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@hookform/devtools": "^4.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/pg": "^8.11.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.12.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.1",
    "postcss": "^8",
    "prettier": "2.5.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
